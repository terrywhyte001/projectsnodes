{
  "openapi": "3.0.0",
  "info": {
    "title": "CRUD API",
    "version": "1.0.0",
    "description": "API documentation for items and users"
  },
  "paths": {
    "/items": {
      "get": {
        "summary": "Get all items",
        "responses": { "200": { "description": "Success" } }
      },
      "post": {
        "summary": "Create a new item",
        "responses": { "201": { "description": "Item created" }, "400": { "description": "Bad request" } }
      }
    },
    "/items/{id}": {
      "get": { "summary": "Get item by ID", "responses": { "200": { "description": "Success" }, "404": { "description": "Not found" } } },
      "put": { "summary": "Update item", "responses": { "200": { "description": "Updated" }, "400": { "description": "Bad request" } } },
      "delete": { "summary": "Delete item", "responses": { "200": { "description": "Deleted" }, "404": { "description": "Not found" } } }
    },
    "/users": {
      "get": { "summary": "Get all users", "responses": { "200": { "description": "Success" } } },
      "post": { "summary": "Create a new user", "responses": { "201": { "description": "Created" }, "400": { "description": "Bad request" } } }
    },
    "/users/{id}": {
      "get": { "summary": "Get user by ID", "responses": { "200": { "description": "Success" }, "404": { "description": "Not found" } } },
      "put": { "summary": "Update user", "responses": { "200": { "description": "Updated" }, "400": { "description": "Bad request" } } },
      "delete": { "summary": "Delete user", "responses": { "200": { "description": "Deleted" }, "404": { "description": "Not found" } } }
    }
  }
}
